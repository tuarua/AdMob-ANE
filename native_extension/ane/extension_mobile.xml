<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.AdMobANE</id>
	<name>AdMob ANE</name>
	<copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
	<versionNumber>1.7.0</versionNumber>
    <platforms>
		<platform name="Android-ARM"> 
           <applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.tuarua.AdMobANE</initializer>
				<finalizer>com.tuarua.AdMobANE</finalizer>
			</applicationDeployment>
        </platform>
		<platform name="Android-x86">
           <applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.tuarua.AdMobANE</initializer>
				<finalizer>com.tuarua.AdMobANE</finalizer>
			</applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAdMobANE.a</nativeLibrary>
                <initializer>TRAMAExtInizer</initializer>
                <finalizer>TRAMAExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libAdMobANE.a</nativeLibrary>
                <initializer>TRAMAExtInizer</initializer>
                <finalizer>TRAMAExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
